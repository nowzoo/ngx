    "demo-date-time-inputs": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo-date-time-inputs",
      "sourceRoot": "projects/demo-date-time-inputs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-date-time-inputs",
            "index": "projects/demo-date-time-inputs/src/index.html",
            "main": "projects/demo-date-time-inputs/src/main.ts",
            "polyfills": "projects/demo-date-time-inputs/src/polyfills.ts",
            "tsConfig": "projects/demo-date-time-inputs/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/demo-date-time-inputs/src/favicon.ico",
              "projects/demo-date-time-inputs/src/assets"
            ],
            "styles": [
              "projects/demo-date-time-inputs/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo-date-time-inputs/src/environments/environment.ts",
                  "with": "projects/demo-date-time-inputs/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo-date-time-inputs:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo-date-time-inputs:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-date-time-inputs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo-date-time-inputs/src/test.ts",
            "polyfills": "projects/demo-date-time-inputs/src/polyfills.ts",
            "tsConfig": "projects/demo-date-time-inputs/tsconfig.spec.json",
            "karmaConfig": "projects/demo-date-time-inputs/karma.conf.js",
            "assets": [
              "projects/demo-date-time-inputs/src/favicon.ico",
              "projects/demo-date-time-inputs/src/assets"
            ],
            "styles": [
              "projects/demo-date-time-inputs/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo-date-time-inputs/tsconfig.app.json",
              "projects/demo-date-time-inputs/tsconfig.spec.json",
              "projects/demo-date-time-inputs/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/demo-date-time-inputs/e2e/protractor.conf.js",
            "devServerTarget": "demo-date-time-inputs:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo-date-time-inputs:serve:production"
            }
          }
        }
      }
    },
