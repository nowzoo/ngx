{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-crumbs": [
        "dist/ngx-crumbs"
      ],
      "ngx-crumbs/*": [
        "dist/ngx-crumbs/*"
      ],
      "ngx-fire": [
        "dist/ngx-fire"
      ],
      "ngx-fire/*": [
        "dist/ngx-fire/*"
      ],
      "ngx-window-title": [
        "dist/ngx-window-title"
      ],
      "ngx-window-title/*": [
        "dist/ngx-window-title/*"
      ],
      "@nowzoo/ngx-route-utils": [
        "dist/ngx-route-utils"
      ],
      "@nowzoo/ngx-route-utils/*": [
        "dist/ngx-route-utils/*"
      ],
      "ngx-sign-in-redirect": [
        "dist/ngx-sign-in-redirect"
      ],
      "ngx-sign-in-redirect/*": [
        "dist/ngx-sign-in-redirect/*"
      ],
      "ngx-form": [
        "dist/ngx-form"
      ],
      "ngx-form/*": [
        "dist/ngx-form/*"
      ],
      "ngx-message": [
        "dist/ngx-message"
      ],
      "ngx-message/*": [
        "dist/ngx-message/*"
      ],
      "ngx-list": [
        "dist/ngx-list"
      ],
      "ngx-list/*": [
        "dist/ngx-list/*"
      ],
      "ngx-firebase-auth": [
        "dist/ngx-firebase-auth"
      ],
      "ngx-firebase-auth/*": [
        "dist/ngx-firebase-auth/*"
      ],
      "@nowzoo/ngx-date-time-inputs": [
        "dist/ngx-date-time-inputs"
      ],
      "@nowzoo/ngx-date-time-inputs/*": [
        "dist/ngx-date-time-inputs/*"
      ],
      "ngx-color-control": [
        "dist/ngx-color-control"
      ],
      "ngx-color-control/*": [
        "dist/ngx-color-control/*"
      ],
      "ngx-bootstrap-popup": [
        "dist/ngx-bootstrap-popup"
      ],
      "ngx-bootstrap-popup/*": [
        "dist/ngx-bootstrap-popup/*"
      ],
      "@nowzoo/ngx-bootstrap-modal": [
        "dist/ngx-bootstrap-modal"
      ],
      "@nowzoo/ngx-bootstrap-modal/*": [
        "dist/ngx-bootstrap-modal/*"
      ]
    }
  }
}